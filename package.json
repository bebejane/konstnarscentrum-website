{
	"name": "konstnarscentrum-website",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "npm run dev:watch",
		"dev:watch": "LOG_GRAPHQL=false next dev & npm run barrel:watch & graphql-codegen --watch",
		"dev:emails": "npx mailing",
		"dev:test": "DATOCMS_ENVIRONMENT=dev NEXT_PUBLIC_DATOCMS_ENVIRONMENT=dev next dev & npm run barrel:watch & graphql-codegen --watch & npm run dev:emails",
		"prebuild": "node ./prebuild",
		"build": "npm run prebuild && next build && npm run postbuild",
		"codegen": "graphql-codegen",
		"start": "next start",
		"lint": "next lint",
		"preview": "npm run build && npm run start",
		"barrel": "node ./lib/scripts/barrel",
		"barrel:watch": "watch 'node ./lib/scripts/barrel' ./components",
		"postbuild": "next-sitemap",
		"importmembers": "tsx --tsconfig ./tsconfig.node.json ./lib/scripts/import-members.tsx",
		"updatemembers": "tsx --tsconfig ./tsconfig.node.json ./lib/scripts/update-members.tsx",
		"updateuploads": "tsx --tsconfig ./tsconfig.node.json ./lib/scripts/update-uploads.tsx",
		"test": "tsx --tsconfig ./tsconfig.node.json ./tests/test.ts",
		"test:emails": "tsx --tsconfig ./tsconfig.node.json ./tests/emails.tsx"
	},
	"exports": {
		"lib/*": "/lib",
		"components/*": "/components",
		"graphql/*": "/graphql",
		"/*": "*"
	},
